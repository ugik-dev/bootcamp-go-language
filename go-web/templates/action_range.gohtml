<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=1, initial-scale=1.0" />
    <title>{{.Title}}</title>
    <link rel="stylesheet" href="app.js" />
  </head>
  <body>
    {{range $index, $element := .Tasks}}
        <h1>{{ $index }}. | {{ $element.Index }}. {{$element.Name}} - {{$element.Description}}</h1>
    {{else}}
        <h1>No Task</h1>
    {{end}}
    <hr>
    <p>di with seperti $element.Name tapi name tidak dipanggil </p>
    {{/* di with seperti $element.Name tapi name tidak dipanggil  */}}
    {{range $index, $element := .Tasks}}
          {{with $element}}
                 <h1>{{ $index }}. | {{ .Index }}.  {{.Name}} - {{.Description}}</h1>
         {{end}}
    {{else}}
        <h1>No Task</h1>
    {{end}}
    <hr>
    {{range .Hobies}}
        <h1>{{.Index}}. {{.Name}} - {{.Description}}</h1>
    {{else}}
        <h1>No Data Hobies</h1>
    {{end}}
   
  </body>
</html>
